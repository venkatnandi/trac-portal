{
  "version": 2,
  "functions": {
    "*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/index.php"
    },
    {
      "src": "/education",
      "dest": "/education.php"
    },
    {
      "src": "/health",
      "dest": "/health.php"
    },
    {
      "src": "/nutrition",
      "dest": "/nutrition.php"
    },
    {
      "src": "/wash",
      "dest": "/wash.php"
    },
    {
      "src": "/css",
      "dest": "/css"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
