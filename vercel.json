{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.php"
    },
    {
      "src": "/education",
      "dest": "/api/education.php"
    },
    {
      "src": "/health",
      "dest": "/api/health.php"
    },
    {
      "src": "/nutrition",
      "dest": "/api/nutrition.php"
    },
    {
      "src": "/wash",
      "dest": "/api/wash.php"
    },
    {
      "src": "/css",
      "dest": "/css"
    },
    {
      "src": "/js/(.*)",
      "dest": "/js/$1"
    },
    {
      "src": "/includes/(.*)",
      "dest": "/includes/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
} 